{
  "manifest_version": 3,
  "name": "Trello Collapse Empty Lanes",
  "version": "1.0.4",
  "description": "Makes wide boards in Trello easier to work with by collapsing empty lanes",
  "icons": {
    "48": "icon-48.png",
    "96": "icon-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{da52ed75-0a82-44f4-ba4d-4b69309a26a3}"
    }
  }

}
